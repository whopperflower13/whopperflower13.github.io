variables: boxes to store values
const: unchange
let: change

number  let a= 10;
string let b= "Minami"
boolean let c= true or false
null let d = null
undefined let e;
object let f = {a: 10, b:"abcd"};

let arr = [1,3,5,7,9];
let strArr = ["a","b", "c"];
they start with position 0 arr[0] =>strArr[0] = "a"
